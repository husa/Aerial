@font-face
  font-family: 'Quicksand'
  src url('./fonts/Quicksand-Light.woff2') format('woff2')
  font-weight 300
  font-style normal

@font-face
  font-family 'Quicksand'
  src url('./fonts/Quicksand.woff2') format('woff2')
  font-weight 400
  font-style normal


html, body, #root
  margin: 0
  padding: 0
  width: 100%
  height: 100%
  overflow: hidden
  font-family: 'Quicksand', Arial, sans-serif
  font-weight: 400
  font-size: 16px
  user-select: none
